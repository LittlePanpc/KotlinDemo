<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="me.danielpan.kotlindemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".kt.NavKtActivity"
            android:label="@string/title_activity_nav_kt"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".BottomNavActivity"
            android:icon="@drawable/ic_menu_send"
            android:label="@string/title_activity_bottom_nav"
            android:roundIcon="@drawable/side_nav_bar"></activity>
        <activity
            android:name=".kt.BottomNavActivity1"
            android:icon="@drawable/ic_menu_send"
            android:label="@string/title_activity_bottom_nav"
            android:roundIcon="@drawable/side_nav_bar"></activity>
    </application>

</manifest>